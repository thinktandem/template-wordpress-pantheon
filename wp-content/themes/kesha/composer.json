{
  "name": "thinktandem/kesha",
  "description": "Starter theme to build a Timber theme",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "jarednova",
      "email": "jared@upstatement.com"
    },
    {
      "name": "John Ouellet",
      "email": "john@thinktandem.io"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "require": {
    "timber/timber": "^1.12"
  }
}
